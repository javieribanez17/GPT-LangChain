<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DB</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/styles.css">
    <link href="/css/card.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/demo3.css">
</head>

<body>
    <%- include("partials/navBar"); -%>
        <div id="general">
            <div id="loaderContainer">
                <div id="loader"></div>
            </div>
            <div class="containerG">
                <br>
                <h4>Escribe el número de caso</h4>
                <div class="containerSearch">
                    <form action="/demo3-search" method="post">
                        <div class="input-group mb-3">
                            <input type="number" name="numCase" class="form-control" style="border-radius: 10px;"
                                placeholder="ej: '1', '2', '3'" aria-label="Recipient's username"
                                aria-describedby="basic-addon2">
                            <div class="input-group-append">
                                <button class="btn btn-outline-secondary consult" style="margin-left: 5px;"
                                    type="submit">Consultar</button>
                            </div>
                        </div>
                    </form>
                </div>
                <% if (paciente) { %>
                    <div class="containerFlexbtn">
                        <div>
                            <h5>Datos extraídos del modelo de inteligenica artificial:</h5>
                        </div>
                        <div>
                            <form action="/activeModel" method="get">
                                <input type="text" name="" id="" hidden>
                                <button style="margin-bottom: 10px;" class="btn btn-primary consult" id="modelButton"
                                    type="submit" name="sendRequest">Analizar</button>
                            </form>
                        </div>
                    </div>

                    <div class="containerTable">
                        <div class="horizontal-table">
                            <% let First=true; %>
                                <% for(jso in paciente) { %>
                                    <% if (First) { %>
                                        <div id="head" class="horizontal-row">
                                            <% for (const info in paciente[jso]) { %>
                                                <div class="horizontal-cell">
                                                    <% if (info !="descripcion" ) { %>
                                                        <%= info %>
                                                            <% } %>
                                                </div>
                                                <% } %>
                                        </div>
                                        <% First=false; %>
                                            <% } %>
                                                <% } %>
                                                    <% for(jso in paciente) { %>
                                                        <div class="horizontal-row">
                                                            <% for (const info in paciente[jso]) { %>
                                                                <div class="horizontal-cell">
                                                                    <% if (info !="descripcion" ) { %>
                                                                        <%= paciente[jso][info] %>
                                                                            <% } %>
                                                                </div>
                                                                <% } %>
                                                        </div>
                                                        <% } %>
                        </div>
                    </div>
                    <% } %>
                        <% if (paciente) { %>
                            <br>
                            <p>
                                <strong>Descripción:</strong>
                                <%= paciente[0].descripcion %>

                            </p>
                            <% } %>
                                <% if (respondModel !="" ) { %>
                                    <h5>Datos extraídos del modelo de inteligenica artificial:</h5>
                                    <div class="containerTable">
                                        <div class="horizontal-table">
                                            <% let First=true; %>
                                                <% for(jso in respondModel) { %>
                                                    <% if (First) { %>
                                                        <div id="head" class="horizontal-row">
                                                            <% for (const info in respondModel[jso]) { %>
                                                                <div class="horizontal-cell">
                                                                    <% if (info !="descripcion" ) { %>
                                                                        <%= info %>
                                                                            <% } %>
                                                                </div>
                                                                <% } %>
                                                        </div>
                                                        <% First=false; %>
                                                            <% } %>
                                                                <% } %>
                                                                    <% for(jso in respondModel) { %>
                                                                        <div class="horizontal-row">
                                                                            <% for (const info in respondModel[jso]) {
                                                                                %>
                                                                                <div class="horizontal-cell">
                                                                                    <% if (info !="descripcion" ) { %>
                                                                                        <%= respondModel[jso][info] %>
                                                                                            <% } %>
                                                                                </div>
                                                                                <% } %>
                                                                        </div>
                                                                        <% } %>
                                        </div>
                                    </div>
                                    <% } %>
            </div>
        </div>
        </div>

        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
            crossorigin="anonymous"></script>
</body>
<script src="/index.js"></script>

</html>